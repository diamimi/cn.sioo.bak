<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.sioo.mapper31.SmsUserConsumeMapper31">


    <select id="findCount31" resultType="java.lang.Integer">
        SELECT count(0) from sms_user_consume
    </select>

    <insert id="insertBatch" parameterType="java.util.List">

        insert into sms_user_consume
        (
        `id`,`uid`,`send`,`unsend`,`date`,`utype`
        )
        values
        <foreach collection="list" item="item" index="index" separator=",">
        (
        #{item.id},#{item.uid},#{item.send},#{item.unsend},#{item.date},#{item.utype}
        )
        </foreach>
    </insert>

</mapper>